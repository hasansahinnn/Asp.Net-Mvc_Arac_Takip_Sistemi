@model List<SiraTakipSistemi.Models.SeferListesi>

<div class="row" style="padding-left:40px;padding-right:40px;padding-top:40px;padding-bottom:0px;height:700px"  >
    <div class="col-md-12 col-sm-12" style="text-align:center">
        <h2>@TempData["FirmaAdi"] Sıra Listesi (@Model.Count)</h2>
    </div>
    <div class="col-md-12 col-sm-12  padding10" style="max-height:80%;margin-top:-5%">
        <table id="tblLocations" class="table- " cellpadding="0" cellspacing="0" border="1" >
            <tr>
                <th>Sıra</th>
                <th>Plaka</th>
                <th>Ad</th>
                <th>Not</th>
                <th>Kayıt Tarihi</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td width="25%">@item.SiraNo</td>
                    <td width="25%">@item.Sofor.Plaka</td>
                    <td width="25%"> @item.Sofor.Ad</td>
                    <td>@item.Not</td>
                    <td width="15%">@item.SeferZamani</td>
                </tr>
            }
        </table>

    </div>



</div>